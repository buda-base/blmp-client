    "buffer": "^6.0.3",
    "debug": "^4.2.0",
    "edtf": "https://github.com/buda-base/edtf.js#rde",
    "fetch-to-curl": "^0.5.2",
    "http-proxy-middleware": "^1.0.6",
    "i18next": "^19.8.9",
