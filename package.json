    "typescript-memoize": "^1.0.0"
  },
  "scripts": {
    "build-css": "yarn run sass src/stylesheets/app.scss:public/app.css -s compressed --update",
    "start-js": "react-scripts start",
    "start": "export NODE_ENV=development && yarn run start-js",
    "build": "npm run build-css && REACT_APP_VERSION=$npm_package_version react-scripts build && rm -rf dist && mv build dist",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "predeploy": "npm run build",
    "lint": "eslint --ignore-path .gitignore .",
    "postinstall": "if [ ! -r src/libs/bvmt/yarn.lock ] ; then git submodule update --init ; fi",
    "prebuild": "git submodule update --remote && yarn run generate-build-meta",
    "generate-build-meta": "./node_modules/react-clear-cache/bin/cli.js",
    "refresh-rde": "cd ../rdf-document-editor/ && yarn build && cd ../blmp-client/node_modules/rdf-document-editor/ && rm -rf dist && cp -rf ../../../rdf-document-editor/dist . && cd ../.. && rm node_modules/.cache -rf"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,tsx,ts}": [
      "prettier --write --config ./.prettierrc",
      "eslint --color --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "dpdm": "^3.10.0",
    "esbuild": "^0.15.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-load-config": "^4.0.1",
    "postcss-url": "^10.1.3",
    "prettier": "^2.7.1",
    "preval.macro": "^5.0.0",
    "react-scripts": "5.0.1",
    "sass": "^1.55.0",
    "ts-loader": "^9.4.1",
    "tsup": "^6.3.0",
    "type-fest": "^3.1.0",
    "typescript": "^4.8.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
